{
  // JNI 动态链接库位置（绝对路径）
  "jni_library" : "",
  // QDUJUDGER 动态链接库位置（绝对路径）
  "judger_library" : "",

  // MySQL 数据库配置
  "mysql_port" : 3306,
  "mysql_user" : "",
  "mysql_pass" : "",
  "mysql_host" : "127.0.0.1",
  "mysql_database" : "",

  // 无任务时的等待时间（毫秒）
  "poll_wait_ms" : 1000,

  // ！！所有目录必须以斜杠结尾！！
  // 临时文件目录，多个评测机同时开启时必须不同
  "temp_path" : "/tmp/",
  // 源代码和测试数据目录
  "source_path" : "/home/OJ/src/",
  "problem_path" : "/home/OJ/p/",
  // 可执行文件文件名，可以随意
  "exe_file_name" : "executable",

  // 编译器二进制文件绝对路径（即可更换版本）
  "c_compiler_absolute" : "/usr/bin/gcc",
  "cpp_compiler_absolute" : "/usr/bin/g++",
  "pascal_compiler_absolute" : "/usr/bin/fpc",

  // 栈大小（兆）
  "stack_size" : 8,
  // 评测沙盒运行用户程序时的身份
  "sandbox_uid" : 1000,
  "sandbox_gid" : 1000
}
